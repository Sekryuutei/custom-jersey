{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.3" }
    },
    "routes": [
        { "src": "/assets/(.*)", "dest": "/public/assets/" },
        { "src": "/css/(.*)", "dest": "/public/css/" },
        { "src": "/js/(.*)", "dest": "/public/js/" },
        { "src": "/storage/public/app/templates(.*)", "dest": "/storage/public/app/templates/" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "public": true,
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://custom-jersey.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "0ez5n.h.filess.io",
        "DB_PORT": "3307",
        "DB_DATABASE": "custom_greatlyled",
        "DB_USERNAME": "custom_greatlyled"
    }
}